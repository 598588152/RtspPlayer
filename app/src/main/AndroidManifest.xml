<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gosuncn.mpa">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".player1.TestPlayer1Activity" />
        <activity
            android:name=".player1.FourScreenActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".player2.TestPlayer2Activity" />
        <activity
            android:name=".player2.TestFourScreenActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".CompareActivity" />
        <activity android:name=".player3.TestPlayer3Activity" />
        <activity android:name=".player3.TestFourPlayer3Activity"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>